        <type full="Twilio\Rest\Chat\V2\Service\Channel\string" namespace="Twilio\Rest\Chat\V2\Service\Channel" name="string"/>
      </param>
      <param description="The SID of the Studio [Flow](https://www.twilio.com/docs/studio/rest-api/flow) to call when an event in `configuration.filters` occurs. Used only when `type` is `studio`." type="string" variable="$configurationFlowSid"/>
      <param description="The number of times to retry the webhook if the first attempt fails. Can be an integer between 0 and 3, inclusive, and the default is 0." type="int" variable="$configurationRetryCount"/>
      <return description="Options builder" type="object">
        <type full="Twilio\Rest\Chat\V2\Service\Channel\CreateWebhookOptions" namespace="Twilio\Rest\Chat\V2\Service\Channel" name="CreateWebhookOptions"/>
